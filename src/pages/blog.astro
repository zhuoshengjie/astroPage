---
import BaseLayout from '../layouts/BaseLayout.astro';
const allPosts = await Astro.glob('../pages/posts/*.md');

const pageTitle = "产品";

---
<BaseLayout pageTitle={pageTitle}>
  <h1>产品介绍</h1>
  <h2>所有产品</h2>
   <ul>
    {allPosts.map((post) => (
      <li>
        <a href={post.url}>{post.frontmatter.title}</a>
        </li>
    ))}
    </ul>
</BaseLayout>

 